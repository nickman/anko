var regexp = import("regexp")
printf("Regexp: %v\n", regexp)

// Create the pattern first
var re = regexp["MustCompile"](`[\s_]`)
println("Compiled")
// Then call Split on the result
var parts = re.Split("foo_bar_baz", -1)
for s in parts {
  println(s)
}


